<!doctype html>
<html lang="en" data-framework="intercoolerjs">
<head>
    <meta charset="utf-8">
    <title>App</title>
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <link rel="stylesheet" href="/css/styles.css">
</head>
<body>
    
    <ul id="chooseLanguage">
            <form>
                <li>
                    <button name="locale" style="color: hotpink; text-decoration: underline; " value="pt_BR">$msg.get("PT_BR")</button>
                </li>
                <li>
                    <button name="locale" style="color: hotpink; text-decoration: underline; "value="en">$msg.get("EN")</button>
                </li>
                <li>
                    <button name="locale" style="color: hotpink; text-decoration: underline; "value="es">$msg.get("ES")</button>
                </li>
            </form>
     </ul>
    
   
    <section id="todoapp" ic-target="this">
    	
    	
        #parse("/velocity/todoList.vm")
    </section>
    <div id="info">
        <p>$msg.get("EDIT")</p>
    </div>
    <script src="/js/vendor/jquery-1.12.4.min.js"></script>
    <script src="/js/vendor/intercooler-0.9.6.min.js"></script>
    <script src="/js/app.js"></script>
</body>
</html>
